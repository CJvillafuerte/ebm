<!-- Modal -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">

                <h4 class="modal-title" id="myModalLabel">Request details</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <fieldset>
                    <div class="control-group">
                        <div class="controls">
                            <label for="requestId">Request Id:</label>
                            <input type="text"  class="form-control" id="requestId" name="requestId" disabled/>
                            <label for="variableId">Variable :</label>
                            <input type="text"  class="form-control" id="variableId" name="variableId" disabled/>
                            <label for="latsId">Lats :</label>
                            <input type="text"  class="form-control" id="latsId" name="latsId" disabled/>
                            <label for="longsId">Longs :</label>
                            <input type="text"  class="form-control" id="longsId" name="longsId" disabled/>
                            <label for="startdate">Start Date :</label>
                            <input type="text"  class="form-control" id="startdate" name="startdate" disabled/>
                            <label for="enddate">End Date :</label>
                            <input type="text"  class="form-control" id="enddate" name="enddate" disabled/>
                            <label for="status">Status :</label>
                            <input type="text"  class="form-control" id="status" name="status" disabled/>
                            <label for="stsmsg">Status Message :</label>
                            <input type="text"  class="form-control" id="stsmsg" name="stsmsg" disabled/>
                        </div>
                    </div>
                </fieldset>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>